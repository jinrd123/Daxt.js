    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "antd": "^5.26.5",
    "axios": "^1.3.6",
    "babel-loader": "^8.0.0",
    "css-loader": "^6.7.3",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
